{
    "presets": [
        [
            "@babel/env",
            {
                "loose": true,
                "modules": false
            }
        ],
        "@babel/react",
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ],
    "env": {
        "commonjs": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ]
        }
    }
}
